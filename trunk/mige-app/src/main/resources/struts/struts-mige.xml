<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
	<package name="mige" namespace="/" extends="default">
		<action name="login" class="com.spark.mige.action.UserLoginAction" method="login">
			<result name="success" type="xslt">
				<param name="location">/WEB-INF/template.xsl</param>
				<param name="exposedValue">userXmlData</param>
			</result>
		</action>
	</package>
</struts>
