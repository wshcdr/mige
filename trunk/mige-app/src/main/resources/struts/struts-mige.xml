<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
	<package name="mige" namespace="/" extends="default">
		<action name="register_user" class="com.spark.mige.action.UserRegisterAction" method="register">
			<result name="success" type="xslt">
				<param name="exposedValue">registerUserXmlDom</param>
			</result>
		</action>
		
		<action name="perfect_inf" class="com.spark.mige.action.UserRegisterAction" method="perfectInfo">
			<result name="success" type="xslt">
				<param name="exposedValue">perfectUserXmlDom</param>
			</result>
		</action>
		
		<action name="login" class="com.spark.mige.action.UserAction" method="login">
			<result name="success" type="xslt">
				<param name="exposedValue">loginFeedback</param>
			</result>
		</action>
	</package>
</struts>
